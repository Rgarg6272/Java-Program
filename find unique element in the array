public class find_unique_element {
    public static void main(String[] args){
        int[] arr = {1, 1, 2, 5, 5};
        int ans = 0;
        for(int i=0; i<arr.length; i++){
            ans = ans ^ arr[i];
        }
        System.out.println("Unique element in the array is: "+ans);
    }
}


// n xor n = 0
// n xor 0 = 1
