import java.util.*;
public class Main {

	public static void main(String[] args) {
	 Scanner sc = new Scanner(System.in);
	 int N = sc.nextInt();
	 int k=65;
	 int l=97;
	 for(int j=1; j<=N; j++){
	     System.out.printf("%c %d %c",l,j,k);
	     System.out.println();
	     l++;
	     k++;
	 }
  }
}
