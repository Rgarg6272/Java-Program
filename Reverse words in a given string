class Solution 
{
    //Function to reverse words in a given string.
    String reverseWords(String S)
    {
        // code here 
        String[] str = S.split("[.]");
        String finalStr = "";
        for(int i=str.length-1; i>=0; i--)
        {
            finalStr = finalStr + str[i];
            if(i!=0){
                if(i<str.length) finalStr +=".";
            }
        }
        return finalStr;
        
    }
}
