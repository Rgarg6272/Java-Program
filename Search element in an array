import java.util.*;
public class Search_element {
    public static void main(String[] args){
        int[] arr = {12,15,6,7};
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        for(int i=0; i<arr.length; i++){
            if(arr[i] == x){
                System.out.println("Element exist at the index number: "+i);
            }
        }
    }
}
