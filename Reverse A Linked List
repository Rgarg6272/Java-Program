import java.util.*;
public class Main
{
	public static void main(String[] args) {
		LinkedList<Integer> list = new LinkedList<>();
		list.add(1);
		list.add(2);
		list.add(3);
		list.add(4);
		for(int i=0; i<list.size(); i++){
		    System.out.print(list.get(i));
		}
		Collections.reverse(list);
		System.out.println(list);
	}
}
